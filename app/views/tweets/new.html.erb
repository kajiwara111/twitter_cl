<%= form_with(model: @tweet, local: true, url: confirm_tweets_path) do |f| %>

    <p><%= alert %></p>
    <div class = 'tweet_content'> 
        <%= f.label :content %>
        <%= f.text_field :content %>
    </div>
    <%= f.submit %>
<% end %>
<%= link_to 'tweet一覧画面へ', tweets_path, method: :get %>

